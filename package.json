{
  "name": "botmou",
  "version": "0.2.0",
  "author": {
    "name": "Damien Mayance",
    "url": "https://dmayance.com/"
  },
  "contributors": [
    {
      "name": "Damien Mayance & Matthieu Oger",
      "url": "https://matthieuoger.com/"
    },
    {
      "name": "Pixelnest Studio",
      "url": "https://pixelnest.io/"
    }
  ],
  "keywords": [
    "hubot",
    "slack",
    "discord",
    "google sheets",
    "bot"
  ],
  "description": "Botmout, a Google Sheets-powered chatbot.",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/valryon/botmou.git"
  },
  "scripts": {
    "start": "hubot --name \"botmou\"",
    "slack": "hubot --name \"botmou\" --adapter slack",
    "discord": "hubot --name \"botmou\" --adapter discord",
    "telegram": "hubot --name \"botmou\" --adapter telegram",
    "test": "eslint ."
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": false
  },
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "space-before-function-paren": [
        "error",
        "never"
      ],
      "object-curly-spacing": [
        "error",
        "never"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js, json}": [
      "eslint"
    ],
    "*.{js, json, css, md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "cleverbot-node": "^0.3.11",
    "cron": "^1.7.1",
    "date-fns": "^1.30.1",
    "generator-hubot": "^0.4.0",
    "google-spreadsheet": "^2.0.7",
    "hubot": "^3.2.0",
    "hubot-auth": "^2.1.0",
    "hubot-diagnostics": "1.0.0",
    "hubot-discord": "^2.1.0",
    "hubot-help": "^1.0.1",
    "hubot-redis-brain": "1.0.0",
    "hubot-slack": "^4.6.0",
    "hubot-telegram": "^1.2.0",
    "lodash": "^4.17.11",
    "yo": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^5.12.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.1",
    "prettier": "^1.16.1"
  }
}
